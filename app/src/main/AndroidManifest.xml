<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="no.clap.higster" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="no.clap.higster.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="no.clap.higster.CafeteriaActivity"
            android:label="@string/title_activity_cafeteria"
            android:parentActivityName="no.clap.higster.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="no.clap.higger.MainActivity" />
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>
